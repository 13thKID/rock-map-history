{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.ts",
    "src/**/*.json"
  ],
  "exclude": [
    "src/**/__tests__/*",
  ],
  "compilerOptions": {
    "lib": [
      "es2022",
      "dom"
    ],
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}